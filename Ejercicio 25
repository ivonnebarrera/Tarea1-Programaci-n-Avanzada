# Ejercicio 25
cadena=(input("Ingrese una frase: "))

cantidad_a = cadena.count('a')
cantidad_b = cadena.count('b')
cantidad_c = cadena.count('c')
cantidad_d = cadena.count('d')
cantidad_e = cadena.count('e')
cantidad_f = cadena.count('f')
cantidad_g = cadena.count('g')
cantidad_h = cadena.count('h')
cantidad_i = cadena.count('i')
cantidad_j = cadena.count('j')
cantidad_k = cadena.count('k')
cantidad_l = cadena.count('l')
cantidad_m = cadena.count('m')
cantidad_n = cadena.count('n')
cantidad_ñ = cadena.count('ñ')
cantidad_o = cadena.count('o')
cantidad_p = cadena.count('p')
