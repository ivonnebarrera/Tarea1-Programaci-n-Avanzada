cantidad_v = cadena.count('v')
cantidad_w = cadena.count('w')
cantidad_x = cadena.count('x')
cantidad_y = cadena.count('y')
cantidad_z = cadena.count('z')

if cantidad_a != 0:
  print('La cantidad de veces que aparece el carácter `a` dentro de la frase es igual a %i.' % (cantidad_a))
