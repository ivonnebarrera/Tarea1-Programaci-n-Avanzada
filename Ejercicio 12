# Ejercicio 12
abcd = ['a','b','c','d','e','f','g','h','i','j',
       'k','l','m','n','ñ','o','p','q','r','s',
       't','u','v','w','x','y','z']
abc = ['y','x','w','v','u','t','s','r','q',
       'p','o','ñ','n','m','l','k','j','i','h',
       'g','f','e','d','c','b','a']

abc = [abc.pop(i-1) for i in range(len(abc), 0, -1) if i%3 != 0]

print(abcd)
print(abc)
